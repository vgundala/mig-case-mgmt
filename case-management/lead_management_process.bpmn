<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_1"
                  targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="Participant_SalesProcess" name="Sales Process" processRef="Process_1" />
  </bpmn:collaboration>
  <bpmn:process id="Process_1" isExecutable="false">
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_System" name="System">
        <bpmn:flowNodeRef>StartEvent_LeadCreated</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ScoreLead</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_SalesManager" name="Sales Manager">
        <bpmn:flowNodeRef>Task_DistributeLeads</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_WorkWithClient</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ApproveConversion</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_LeadConverted</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_SalesPerson" name="Sales Person">
        <bpmn:flowNodeRef>Task_NurtureLead</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_ValueCheck</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_EscalateToManager</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_NurturingComplete</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_LeadCreated" name="Lead Created">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- System Tasks -->
    <bpmn:task id="Task_ScoreLead" name="Score Lead">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>

    <!-- Manager Tasks -->
    <bpmn:task id="Task_DistributeLeads" name="Distribute New Leads">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>

    <!-- Sales Person Tasks -->
    <bpmn:task id="Task_NurtureLead" name="Nurture Lead">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>

    <!-- Gateway -->
    <bpmn:exclusiveGateway id="Gateway_ValueCheck" name="Potential Value > $1M?">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_Escalate</bpmn:outgoing>
      <bpmn:outgoing>Flow_Standard</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- Escalation Path -->
    <bpmn:task id="Task_EscalateToManager" name="Escalate to Manager">
      <bpmn:incoming>Flow_Escalate</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_WorkWithClient" name="Manager Works with Client">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_ApproveConversion" name="Approve Conversion">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:task>

    <!-- End Events -->
    <bpmn:endEvent id="EndEvent_LeadConverted" name="Lead Converted">
      <bpmn:incoming>Flow_7</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:endEvent id="EndEvent_NurturingComplete" name="Nurturing Complete">
      <bpmn:incoming>Flow_Standard</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_LeadCreated" targetRef="Task_ScoreLead" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_ScoreLead" targetRef="Task_DistributeLeads" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_DistributeLeads" targetRef="Task_NurtureLead" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_NurtureLead" targetRef="Gateway_ValueCheck" />
    <bpmn:sequenceFlow id="Flow_Escalate" name="Yes" sourceRef="Gateway_ValueCheck" targetRef="Task_EscalateToManager" />
    <bpmn:sequenceFlow id="Flow_Standard" name="No" sourceRef="Gateway_ValueCheck" targetRef="EndEvent_NurturingComplete" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_EscalateToManager" targetRef="Task_WorkWithClient" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_WorkWithClient" targetRef="Task_ApproveConversion" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_ApproveConversion" targetRef="EndEvent_LeadConverted" />

  </bpmn:process>

  <!-- Diagram Information (not strictly necessary for compatibility, but good practice) -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collaboration_1">
      <!-- Lanes -->
      <bpmndi:BPMNShape id="Participant_SalesProcess_di" bpmnElement="Participant_SalesProcess" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="1200" height="600" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_System_di" bpmnElement="Lane_System" isHorizontal="true">
        <dc:Bounds x="190" y="80" width="1170" height="125" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_SalesManager_di" bpmnElement="Lane_SalesManager" isHorizontal="true">
        <dc:Bounds x="190" y="205" width="1170" height="250" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_SalesPerson_di" bpmnElement="Lane_SalesPerson" isHorizontal="true">
        <dc:Bounds x="190" y="455" width="1170" height="225" />
      </bpmndi:BPMNShape>

      <!-- Flow Nodes -->
      <bpmndi:BPMNShape id="StartEvent_LeadCreated_di" bpmnElement="StartEvent_LeadCreated">
        <dc:Bounds x="242" y="122" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="228" y="165" width="65" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ScoreLead_di" bpmnElement="Task_ScoreLead">
        <dc:Bounds x="330" y="100" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_DistributeLeads_di" bpmnElement="Task_DistributeLeads">
        <dc:Bounds x="480" y="290" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_NurtureLead_di" bpmnElement="Task_NurtureLead">
        <dc:Bounds x="630" y="520" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_ValueCheck_di" bpmnElement="Gateway_ValueCheck" isMarkerVisible="true">
        <dc:Bounds x="785" y="535" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="768" y="592" width="84" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_EscalateToManager_di" bpmnElement="Task_EscalateToManager">
        <dc:Bounds x="890" y="520" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_WorkWithClient_di" bpmnElement="Task_WorkWithClient">
        <dc:Bounds x="890" y="290" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ApproveConversion_di" bpmnElement="Task_ApproveConversion">
        <dc:Bounds x="1040" y="290" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_LeadConverted_di" bpmnElement="EndEvent_LeadConverted">
        <dc:Bounds x="1192" y="312" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1171" y="355" width="78" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_NurturingComplete_di" bpmnElement="EndEvent_NurturingComplete">
        <dc:Bounds x="892" y="622" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="866" y="665" width="89" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Sequence Flows -->
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="278" y="140" />
        <di:waypoint x="330" y="140" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="430" y="140" />
        <di:waypoint x="530" y="140" />
        <di:waypoint x="530" y="290" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <di:waypoint x="530" y="370" />
        <di:waypoint x="530" y="560" />
        <di:waypoint x="630" y="560" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <di:waypoint x="730" y="560" />
        <di:waypoint x="785" y="560" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Escalate_di" bpmnElement="Flow_Escalate">
        <di:waypoint x="835" y="560" />
        <di:waypoint x="890" y="560" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="855" y="542" width="18" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Standard_di" bpmnElement="Flow_Standard">
        <di:waypoint x="810" y="585" />
        <di:waypoint x="810" y="640" />
        <di:waypoint x="892" y="640" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="820" y="610" width="15" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_5_di" bpmnElement="Flow_5">
        <di:waypoint x="940" y="520" />
        <di:waypoint x="940" y="370" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_6_di" bpmnElement="Flow_6">
        <di:waypoint x="990" y="330" />
        <di:waypoint x="1040" y="330" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_7_di" bpmnElement="Flow_7">
        <di:waypoint x="1140" y="330" />
        <di:waypoint x="1192" y="330" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>